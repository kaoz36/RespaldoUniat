<resources xmlns:android="http://schemas.android.com/apk/res/android">

     <style name="CustomWindowTitleBackground">
        <item name="android:background">#B9C126</item>
    </style>
 
    <style name="titleText" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">35sp</item>
        <item name="android:gravity">center_horizontal</item>
        
    </style>
    
     <style name="titleBlackText" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">35sp</item>
        <item name="android:gravity">center_horizontal</item>
        
    </style>
    
    <style name="captionText">
        <item name="android:textColor">#275196</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="grayHorizontarSeparator" >
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2dip</item>
        <item name="android:background">#474928</item>
    </style>
    
    
    <style name="menuButton" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">32sp</item>
    </style>
    
    <style name="CustomTheme" parent="android:Theme">
        <item name="android:windowTitleSize">47dip</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
    
    <style name="blackinfotext" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_marginRight">7dp</item>
        <item name="android:layout_marginLeft">7dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="blueinfotext" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#3F84BE</item>
        <item name="android:textSize">20sp</item>
    </style>
    
    
    <style name="blackforminput" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:background">@drawable/commoneditskin</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:paddingRight">7dp</item>
        <item name="android:singleLine">true</item> 
    </style>
    
    <style name="cotizadorforminput" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@drawable/cotizadoreditskin</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:singleLine">true</item> 
    </style>
    
    <style name="cotizadorformenable" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">17sp</item>
        <item name="android:background">@drawable/cotizadorenableskin</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:singleLine">true</item> 
    </style>
    
    <style name="cotizadorheadertext" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingRight">14dp</item>
    </style>
    
    <style name="cotizadorrow">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
    
    <style name="Divider">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">1dp</item>
	    <item name="android:background">#FFB0AFB5</item>
	</style>
    
    <style name="tabButton" >
        <item name="android:background">@drawable/tabskin</item>
        <item name="android:layout_height">162dp</item>
        <item name="android:layout_width">47dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:gravity">center_vertical|center_horizontal|bottom</item>
    </style>
    
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>